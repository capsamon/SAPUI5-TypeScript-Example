{
	"_version": "1.9.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "Reuse",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.52.7"
		},
		"dataSources": {
            "reusecap-srv":{
                "uri": "/api/v2/re-use/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "object1",
					"action": "action1",
					"title": "Reuse",
					"indicatorDataSource": {
						"dataSource": "reusecap-srv",
						"path": ""
					}
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"fullWidth": true,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.3.0",
		"config": {
			"fullWidth": true
		},
		"services": {
            "ShellUIService": {
                "factoryName": "sap.ushell.ui5service.ShellUIService",
                "lazy": false,
                "settings": {
                    "setHierarchy": "auto",
                    "setTitle": "auto"
                }
            }
        },
		"handleValidation": true,
		"rootView": {
			"viewName": "Reuse.view.MainView",
			"type": "XML",
			"async": true,
			"id": "MainView"
		},
		"dependencies": {
			"minUI5Version": "1.50.0",
			"libs": {
				"sap.m": {},
				"sap.tnt": {
					"minVersion": ""
				},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Reuse.i18n.i18n"
				},
				"preload": true
			},
			"": {
                "type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "reusecap-srv",
				"preload": true
			},
			"overviewVariantManagement": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"liveDataModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"targets": {
				"Overview": {
					"viewId": "Overview",
					"viewName": "Overview",
					"viewLevel": 1,
					"controlAggregation": "pages",
					"title": "Re-use scenarios"
				},
				"Details": {
					"viewId": "Details",
					"viewName": "Details",
					"viewLevel": 2,
					"controlAggregation": "pages",
					"title": "Re-use details"
				}
			},
			"routes": [
				{
					"name": "Overview",
					"pattern": "",
					"target": [
						"Overview"
					]
				}, {
					"name": "Details",
					"pattern": "/Details/:TopPlant:/:ChildPlant:/:ChildMaterial:/:TopMaterial:/:TopEquipment:/:ChildEquipment:/:isLive:/",
					"target": [
						"Details"
					]
				}
			],
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "Reuse.view",
				"controlId": "app",
				"targetAggregation": "pages",
				"async": true,
				"clearControlAggregation": false
			}
		}
	}
}