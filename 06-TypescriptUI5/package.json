{
    "name": "TypescriptDemoLibrary",
    "version": "0.0.1",
    "description": "",
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@sap/ui5-builder-webide-extension": "1.0.x",
        "@sap/ux-ui5-tooling": "1",
        "@sapui5/ts-types-esm": "^1.112.0",
        "@types/jquery": "^3.5.16",
        "@ui5/cli": "^2.11.1",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "babel-preset-transform-ui5": "^7.0.5",
        "concurrently": "^7.6.0",
        "npm-run-all": "^4.1.5",
